# Proyecto_final

#include "tabla.h"
#include "ui_tabla.h"

Tabla::Tabla(QWidget *parent) :
    QMainWindow(parent),
    ui(new Ui::Tabla)
{
    ui->setupUi(this);
    setWindowTitle("BUSCAMINAS");
    n=0;
    ui->Jugar->click();
}

Tabla::~Tabla()
{
    delete ui;
}

void Tabla::on_Jugar_clicked()
{
    int f=0,c=0;
    n=0;

    srand(time(NULL));

    for (int i=0;i<12;i++){
        for (int j=0;j<12;j++){
            tabla[i][j]= 0;
        }
    }
    for (int i=0;i<15;i++){
        f = 1 + rand() % ((9+1)-0);
        c = 1 + rand() % ((9+1)-0);
        if (tabla[f][c]==0){
            tabla[f][c] = 9;
        }
        else{
            f = 1 + rand() % ((9+1)-0);
            c = 1 + rand() % ((9+1)-0);
            if (tabla[f][c]==0){
                tabla[f][c] = 9;
            }
        }
    }
    for (int i=0;i<12;i++){
        for (int j=0;j<12;j++){
            if (i>0 && i<11 && j>0 && j<11){
                if(tabla[i][j]==9){

                }
                else{
                    int a = 0;
                    if (tabla[i-1][j-1] == 9){
                        a = a + 1;
                    }
                    if (tabla[i-1][j] == 9){
                        a = a + 1;
                    }
                    if (tabla[i-1][j+1] == 9){
                        a = a + 1;
                    }
                    if (tabla[i][j-1] == 9){
                        a = a + 1;
                    }
                    if (tabla[i][j+1] == 9){
                        a = a + 1;
                    }
                    if (tabla[i+1][j-1] == 9){
                        a = a + 1;
                    }
                    if (tabla[i+1][j] == 9){
                        a = a + 1;
                    }
                    if (tabla[i+1][j+1] == 9){
                        a = a + 1;
                    }
                    tabla[i][j] = a;
                }
            }

        }
    }

    for (int i=0;i<12;i++){
        for (int j=0;j<12;j++){
            ui->tableWidget->setItem(i,j,new QTableWidgetItem(QString::number(tabla[i][j])));
        }
    }
    ui->Barrera->hide();
    ui->pushButton11->show();
    ui->pushButton12->show();
    ui->pushButton13->show();
    ui->pushButton14->show();
    ui->pushButton15->show();
    ui->pushButton16->show();
    ui->pushButton17->show();
    ui->pushButton18->show();
    ui->pushButton19->show();
    ui->pushButton110->show();
    ui->pushButton21->show();
    ui->pushButton22->show();
    ui->pushButton23->show();
    ui->pushButton24->show();
    ui->pushButton25->show();
    ui->pushButton26->show();
    ui->pushButton27->show();
    ui->pushButton28->show();
    ui->pushButton29->show();
    ui->pushButton210->show();
    ui->pushButton31->show();
    ui->pushButton32->show();
    ui->pushButton33->show();
    ui->pushButton34->show();
    ui->pushButton35->show();
    ui->pushButton36->show();
    ui->pushButton37->show();
    ui->pushButton38->show();
    ui->pushButton39->show();
    ui->pushButton310->show();
    ui->pushButton41->show();
    ui->pushButton42->show();
    ui->pushButton43->show();
    ui->pushButton44->show();
    ui->pushButton45->show();
    ui->pushButton46->show();
    ui->pushButton47->show();
    ui->pushButton48->show();
    ui->pushButton49->show();
    ui->pushButton410->show();
    ui->pushButton51->show();
    ui->pushButton52->show();
    ui->pushButton53->show();
    ui->pushButton54->show();
    ui->pushButton55->show();
    ui->pushButton56->show();
    ui->pushButton57->show();
    ui->pushButton58->show();
    ui->pushButton59->show();
    ui->pushButton510->show();
    ui->pushButton61->show();
    ui->pushButton62->show();
    ui->pushButton63->show();
    ui->pushButton64->show();
    ui->pushButton65->show();
    ui->pushButton66->show();
    ui->pushButton67->show();
    ui->pushButton68->show();
    ui->pushButton69->show();
    ui->pushButton610->show();
    ui->pushButton71->show();
    ui->pushButton72->show();
    ui->pushButton73->show();
    ui->pushButton74->show();
    ui->pushButton75->show();
    ui->pushButton76->show();
    ui->pushButton77->show();
    ui->pushButton78->show();
    ui->pushButton79->show();
    ui->pushButton710->show();
    ui->pushButton81->show();
    ui->pushButton82->show();
    ui->pushButton83->show();
    ui->pushButton84->show();
    ui->pushButton85->show();
    ui->pushButton86->show();
    ui->pushButton87->show();
    ui->pushButton88->show();
    ui->pushButton89->show();
    ui->pushButton810->show();
    ui->pushButton91->show();
    ui->pushButton92->show();
    ui->pushButton93->show();
    ui->pushButton94->show();
    ui->pushButton95->show();
    ui->pushButton96->show();
    ui->pushButton97->show();
    ui->pushButton98->show();
    ui->pushButton99->show();
    ui->pushButton910->show();
    ui->pushButton101->show();
    ui->pushButton102->show();
    ui->pushButton103->show();
    ui->pushButton104->show();
    ui->pushButton105->show();
