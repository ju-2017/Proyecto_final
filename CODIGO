    ui->pushButton109->show();
    ui->pushButton1010->show();
}

void Tabla::on_Salir_clicked()
{
    close();
}

void Tabla::on_pushButton11_clicked()
{
    ui->pushButton11->hide();
    if (tabla[1][1] == 9){
        QMessageBox *perder= new QMessageBox(this);
        perder->setWindowTitle("PERDER");
        perder->setText("¡LO SIENTO! PERDISTE");
        perder->setStandardButtons(QMessageBox::Ok);
        perder->setDefaultButton(QMessageBox::Ok);
        int vbla = perder->exec();
        switch (vbla){
        case QMessageBox::Ok:
            perder->hide();
            ui->Barrera->show();
            break;
        }
    }
    else{
        n = n +1;
    }


    if (n==85){
        QMessageBox *ganar = new QMessageBox(this);
        ganar->setWindowTitle("GANAR");
        ganar->setText("¡FELICITACIONES! GANASTE");
        ganar->setStandardButtons(QMessageBox::Ok);
        ganar->setDefaultButton(QMessageBox::Ok);
        int vbla = ganar->exec();
        switch (vbla){
        case QMessageBox::Ok:
            ganar->hide();
            ui->Barrera->show();
            break;
        }
    }
}

void Tabla::on_pushButton12_clicked()
{
    ui->pushButton12->hide();
    if (tabla[1][2] == 9){
        QMessageBox *perder = new QMessageBox(this);
        perder->setWindowTitle("PERDER");
        perder->setText("¡LO SIENTO! PERDISTE");
        perder->setStandardButtons(QMessageBox::Ok);
        perder->setDefaultButton(QMessageBox::Ok);
        int vbla = perder->exec();
        switch (vbla){
        case QMessageBox::Ok:
            ui->Barrera->show();
            perder->hide();
            break;
        }
    }
    else{
        n = n +1;
    }

    if (n==85){
        QMessageBox *ganar = new QMessageBox(this);
        ganar->setWindowTitle("GANAR");
        ganar->setText("¡FELICITACIONES! GANASTE");
        ganar->setStandardButtons(QMessageBox::Ok);
        ganar->setDefaultButton(QMessageBox::Ok);
        int vbla = ganar->exec();
        switch (vbla){
        case QMessageBox::Ok:
            ui->Barrera->show();
            ganar->hide();
            break;
        }
    }
}

